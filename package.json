{
  "name": "biojs-vis-protein-viewer",
  "description": "Displays protein domains",
  "version": "0.1.0",
  "homepage": "https://github.com/ragingsquirrel3/biojs-vis-protein-viewer",
  "author": {
    "name": "Travis Sheppard",
    "email": "travissheppard.1988@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ragingsquirrel3/biojs-vis-protein-viewer.git"
  },
  "bugs": {
    "url": "https://github.com/ragingsquirrel3/biojs-vis-protein-viewer/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ragingsquirrel3/biojs-vis-protein-viewer/blob/master/LICENSE"
    }
  ],
  "main": "lib/index.js",
  "style": "css/*.css", 
  "scripts": {
		"test": "mocha",
		"build": "mkdirp build && browserify -r ./:biojs-vis-protein-viewer -o build/viewer.js",
		"build-browser": "npm run build",
		"install": "npm run build && npm run css",
		"watch": "watchify -r ./:biojs-vis-protein-viewer -v -o build/viewer.js",
		"w": "prunner \"npm run sniper\" \"npm run watch\" \"npm run watch-css\"",
		"sniper": "sniper .",
		"css": "parcelify ./ -c build/bundle.css",
		"watch-css": "parcelify -w ./ -c build/bundle.css --loglevel verbose"
  },
  "dependencies": {},
  "devDependencies": {
		"mkdirp": "^0.5.0",
		"browserify": "6.x",
		"sniper": "0.x",
		"watchify": "^1.0.6",
		"prunner": "1.x",
		"parcelify": "0.x",
		"chai": "1.x",
		"mocha": "1.x"
  }, 
  "sniper": {
    "js": [ "/build/viewer.js"],
    "css": ["/build/bundle.css"], 
    "snippets": ["examples"],
    "first": "simple"
   }, 
  "keywords": ["biojs","protein","d3","biojs"]
}
